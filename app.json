{
    "expo": {
        "name": "pro-cris-v2",
        "slug": "pro-cris-v2",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./src/assets/app/icon.png",
        "splash": {
            "image": "./src/assets/app/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
